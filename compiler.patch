+  let rparen = pack (maybe (char ')')) (fun _->Nil) in
+  let lbrack = char '[' in
+  let rbrack = pack (maybe (char ']')) (fun _->Nil) in
+  let sexp1 = plus not_closed_reader in
+  let sexp2 = not_closed_reader in
